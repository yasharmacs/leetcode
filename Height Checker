class Solution {
    public int heightChecker(int[] heights) {
        int[] arr=new int[heights.length];
        int count=0;
        for(int i=0;i<heights.length;i++)
        {
            arr[i]=heights[i];
        }
        Arrays.sort(heights);
        for(int j=0;j<heights.length;j++)
        {
            if(arr[j]!=heights[j])
            {
                count++;
            }
        }
        return count;
    }
}
